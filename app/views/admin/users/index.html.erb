<header>
  <h1 class="title">List of Users</h1>
  
    <div class="actions">
      <%= link_to icon("fas", "user-plus", "New User"),
          new_admin_user_path, class: "button is-primary is-rounded" %>  
    </div>
</header>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user, [:admin, user] %></li>
  <% end %>
</ul>